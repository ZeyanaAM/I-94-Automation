function swapDisplay( selected, event )
{
	if (typeof event != 'undefined' && event.keyCode != 13)
	{
		return;
	}
	var list = ["retrieve","faq"];
	var el = document.getElementById(selected);
	el.style.display="block";
	el = document.getElementById(selected + "Tab");
	el.style.backgroundColor="white";	
	el.style.color="#001E5A";

	if(selected == "retrieve")
	{
		el = document.getElementById("bluebar1");
		el.style.visibility="hidden";
		
		el = document.getElementById("bluebar2");
		el.style.visibility="visible";

		window.document.title = nonFaqTitle;
	}
	else
	{
		el = document.getElementById("bluebar2");
		el.style.visibility="hidden";
		
		el = document.getElementById("bluebar1");
		el.style.visibility="visible";

		window.document.title = 'I-94 Frequently Asked Questions';
	}
	
	for( var i = 0; i < list.length; i++ )
	{
		if( list[i] != selected )
		{
			el = document.getElementById( list[i] );
			el.style.display="none";
			el = document.getElementById(list[i] + "Tab");
			el.style.backgroundColor="#BBCAE9";
		}
	}
}

